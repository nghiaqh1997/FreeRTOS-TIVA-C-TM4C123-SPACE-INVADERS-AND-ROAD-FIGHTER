#include "include.h"
#include "Images.h"
#include "Task_def.h"
#include "Nokia5110/SPI.h"
#include "sprite.h"
#include "FSM/FSM.h"
void vTask5(void *pvParameters){
    portTickType xLastTaskWakeTime = xTaskGetTickCount();
    int r = 1 + rand() % (15 + 1 - 1);
    while(1){
        if(Missile[r].life==0){
            Missile[r].life = 1;
        }
        vTaskDelay(pdMS_TO_TICKS(500));
    }
}



