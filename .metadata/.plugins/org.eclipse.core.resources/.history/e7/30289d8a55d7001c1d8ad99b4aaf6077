#ifndef SPRITE_H_
#define SPRITE_H_
typedef enum {dead,live} life;
typedef struct {
    unsigned char x;
    unsigned char y;
    life life;
    uint8_t hp;
    const unsigned char *explode;
    const unsigned char *image;
    const unsigned char *block;
}sprite;
sprite Player;
sprite Enemy[14];
sprite Bunker[2];



#endif /* SPRITE_H_ */
